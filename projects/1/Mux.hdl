/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=notsel);
    Nand(a=a, b=notsel, out=iout1);
    Nand(a=sel, b=b, out=iout3);
    Nand(a=iout1, b=iout3, out=out);
}
